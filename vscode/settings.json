{
    "editor.formatOnPaste": true,
    "explorer.confirmDelete": false,
    "files.insertFinalNewline": true,
    "trailing-spaces.highlightCurrentLine": false,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Plastic - deprioritised punctuation",
    "workbench.sideBar.location": "right",
    "workbench.statusBar.visible": true,
    "workbench.iconTheme": "vscode-great-icons",
    "workbench.colorCustomizations": {
        "terminal.ansiCyan": "#9494CC",
        "terminal.ansiBrightCyan": "#9494CC",
        "terminal.ansiGreen": "#98C379",
        "terminal.ansiBrightGreen": "#98C379",
        "terminal.ansiBlue": "#61AFEF",
        "terminal.ansiBrightBlue": "#61AFEF",
        "terminal.ansiRed": "#e06c75",
        "terminal.ansiBrightRed": "#e06c75",
        "terminal.ansiBrightYellow": "#D19A66",
        "terminal.ansiYellow": "#D19A66"
    },
    "editor.minimap.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.hovers.currentLine.enabled": false,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "dist": true
    },
    "gitlens.codeLens.scopes": [
        "containers",
        "document",
        "blocks"
    ],
    "gitlens.codeLens.enabled": false,
    "explorer.confirmDragAndDrop": false,
    "gitlens.keymap": "chorded",
    "diffEditor.ignoreTrimWhitespace": false,
    "eslint.enable": false,
    "editor.lineHeight": 18,
    "editor.fontFamily": "InconsolataGo, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 15,
    "gitlens.currentLine.enabled": false,
    "vim.visualstar": true,
    "vim.useSystemClipboard": true,
    "terminal.integrated.fontSize": 15,
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-e>": false,
        "<C-f>": false,
        "<C-b>": false,
        "<C-n>": false,
        "<C-p>": false
    },
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.historyExplorer.enabled": true,
    "workbench.startupEditor": "newUntitledFile"
}
